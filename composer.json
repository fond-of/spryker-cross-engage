{
    "name": "fond-of-spryker/cross-engage",
    "description": "n/a",
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Fischer",
            "email": "pascal.fischer@fondof.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "spryker/glossary": "^3.0.0",
        "spryker/kernel": "^3.0.0",
        "spryker/zed-request": "^3.0.0",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.17.14",
        "codeception/codeception": "^5.0.0",
        "fond-of-codeception/spryker": "dev-feature/php8 as 1.0.0",
        "mikey179/vfsstream": "^1.6",
        "sebastian/phpcpd": "^6.0.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FondOfSpryker\\": "src/FondOfSpryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FondOfSpryker\\": "tests/FondOfSpryker/",
            "Generated\\": "tests/Generated/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
